<?xml version="1.0" encoding="utf-8"?>
<ScrollView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:background="@drawable/white_background">

    <!-- TODO: Update blank fragment layout -->

    <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


        <TextView
                android:text="Svevestøv"
                android:layout_width="325dp"
                android:layout_height="45dp"
                android:id="@+id/text_svevestovInfo"
                android:fontFamily="sans-serif-medium"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:textSize="36sp"
                android:textColor="@color/FHIblaaVanlig"
                app:layout_constraintVertical_bias="0.041" app:layout_constraintHorizontal_bias="0.348"/>

        <TextView
                android:id="@+id/text_sistEndretHelse"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sist endret: 9. Mai 2019 av Silje M. Flaaten"
                android:textSize="13dp"
                android:layout_below="@id/text_fragPartikler"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintHorizontal_bias="0.094"
                app:layout_constraintVertical_bias="0.128"/>

        <TextView
                android:id="@+id/txt_svev_info"
                android:layout_width="378dp"
                android:layout_height="109dp"
                android:text="@string/text_svev_info"
                android:textSize="15dp"
                android:textStyle="italic"
                android:textColor="@color/FHIblaaVanlig"
                android:layout_below="@id/text_sistEndretHelse"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintHorizontal_bias="0.484"
                app:layout_constraintVertical_bias="0.2"/>


        <TextView
                android:id="@+id/text_svevTittel2"
                android:layout_width="250dp"
                android:layout_height="24dp"
                android:text="@string/text_svev_tittel_1"
                android:textStyle="bold"
                android:textSize="16dp"
                android:textColor="@color/FHIblaaVanlig"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintHorizontal_bias="0.099"
                app:layout_constraintVertical_bias="0.346"/>

        <TextView
                android:id="@+id/txt_svev_del2"
                android:layout_width="371dp"
                android:layout_height="184dp"
                android:text="@string/text_svev_1"
                android:textSize="15dp"
                android:textColor="@color/FHIblaaVanlig"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintHorizontal_bias="0.4"
        />


        <!-- Må ha dette textViewet, hvis ikke klikker alt. Herifra-->
        <TextView
                android:id="@+id/txt_tiltak_del2"
                android:layout_width="405dp"
                android:layout_height="703dp"
                android:text=""
                android:textSize="15dp"
                android:textColor="@color/FHIblaaVanlig"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintHorizontal_bias="0.4"
                app:layout_constraintVertical_bias="1.0"/>

        <!-- Ned hit-->


        <TextView
                android:id="@+id/text_svevTittel3"
                android:layout_width="250dp"
                android:layout_height="24dp"
                android:text="@string/text_svev_tittel_2"
                android:textStyle="bold"
                android:textSize="16dp"
                android:textColor="@color/FHIblaaVanlig"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintHorizontal_bias="0.099"
                app:layout_constraintVertical_bias="0.673"/>

        <TextView
                android:id="@+id/txt_svev_del3"
                android:layout_width="371dp"
                android:layout_height="213dp"
                android:text="@string/text_svev_2"
                android:textSize="15dp"
                android:textColor="@color/FHIblaaVanlig"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintHorizontal_bias="0.4"
                app:layout_constraintVertical_bias="1.0"/>


    </android.support.constraint.ConstraintLayout>



</ScrollView>